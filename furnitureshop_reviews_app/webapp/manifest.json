{
	"_version": "1.8.0",
	"sap.app": {
		"id": "furnitureshop_reviews_app.furnitureshop_reviews_app",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "furnitureshop_reviews_app.furnitureshop_reviews_app.view.products_list",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.38.34",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "furnitureshop_reviews_app.furnitureshop_reviews_app.i18n.i18n"
				}
			},
			"selectedProductModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"userInfo": {
				"type": "sap.ui.model.json.JSONModel"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "furnitureshop_reviews_app.furnitureshop_reviews_app.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "Routeproducts_list",
					"pattern": "Routeproducts_list",
					"target": [
						"Targetproducts_list"
					]
				},
				{
					"name": "Routeproduct_details",
					"pattern": "Routeproduct_details",
					"target": [
						"product_details"
					]
				}
			],
			"targets": {
				"Targetproducts_list": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "products_list",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "furnitureshop_reviews_app.furnitureshop_reviews_app.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl"
				},
				"product_details": {
					"viewType": "XML",
					"viewName": "product_details"
				}
			}
		}
	}
}